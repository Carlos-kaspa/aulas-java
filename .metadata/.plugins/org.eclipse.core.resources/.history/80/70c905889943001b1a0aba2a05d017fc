
public class SwitchCase {

	int mes = 1;
	
	
	switch (mes) {
	
	case 1:
		System.out.println("Você escolheu o mes Janeiro");
		break;
	case 2:
		System.out.println(" Você escolheu fevereiro");
		break;
	case 3:
		System.out.println(" você escolheu março");
		break;
	case 4:
		System.out.println(" você escolheu abril");
		break;
		
	}
	
}
