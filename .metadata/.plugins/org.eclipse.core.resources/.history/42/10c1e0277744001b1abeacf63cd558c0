
public class testaBytebankv2 {

	public static void main(String[] args) {
		
		Conta conta = new Conta("Carlos Lima");
		
		System.out.print(conta.getTitular().nome + " -> ");
		System.out.println(conta.getAgencia() + "/" + conta.getConta() + "-" + conta.getDac());
		
		conta.deposita(200, conta);
		
		
		
		Conta conta2 = new Conta("Monique Guimarães");
		conta2.saca(100);
		System.out.println(conta2.getSaldo());
		
			
	}
}
